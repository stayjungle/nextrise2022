{
  "name": "nextrise2022",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev --host 0.0.0.0",
    "build": "bins/fetch.sh && svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest --run",
    "test-files": "vitest --run related",
    "test:watch": "vitest",
    "lint": "npm run lint:js-files -- . && npm run lint:css-files -- \"src/**/*.{css,svelte}\"",
    "lint:js-files": "prettier --check --plugin-search-dir=.",
    "lint:css-files": "stylelint --config .stylelintrc.json",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.34",
    "@sveltejs/kit": "next",
    "@types/d3": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "autoprefixer": "^10.4.7",
    "d3-dsv": "^3.0.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.14",
    "postcss-html": "^1.4.1",
    "postcss-load-config": "^4.0.1",
    "postcss-nested": "^5.0.6",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "stylelint": "^14.9.1",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.6",
    "tslib": "^2.3.1",
    "typescript": "^4.7.2",
    "vitest": "^0.14.2",
    "vitest-svelte-kit": "^0.0.6"
  },
  "type": "module"
}
